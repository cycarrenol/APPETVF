<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".userinterfaz.UserInterfaz">


    <Button
        android:id="@+id/createq"
        android:layout_width="63dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/IdDesconectar"
        android:layout_marginStart="106dp"
        android:background="#989898"
        android:text="Guardar"
        android:textColor="#FFFFFF"
        android:textSize="10sp" />


    <Button
        android:id="@+id/Begin"
        android:layout_width="52dp"
        android:layout_height="32dp"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/textoTXT"
        android:layout_marginTop="224dp"
        android:background="#436827"
        android:text="iniciar"
        android:textColor="#FFFFFF" />


    <EditText
        android:id="@+id/ETSeparacion"
        android:layout_width="109dp"
        android:layout_height="54dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="47dp"
        android:layout_marginTop="81dp"
        android:background="#FFFFFF"
        android:digits="0123456789."
        android:hapticFeedbackEnabled="false"
        android:hint="separaciÃ³n"
        android:inputType="numberDecimal"
        android:textColor="#436827"
        android:textColorHint="#436827"
        android:textSize="15sp" />


    <EditText
        android:id="@+id/NUMELE"
        android:layout_width="wrap_content"
        android:layout_height="51dp"
        android:layout_alignStart="@+id/ETSeparacion"
        android:layout_alignTop="@+id/EnviarNumele"
        android:background="#FFFFFF"
        android:digits="0123456789."
        android:hapticFeedbackEnabled="false"
        android:hint=" num electrodos"
        android:inputType="numberDecimal"
        android:textColor="#436827"
        android:textColorHint="#436827"
        android:textSize="15sp" />

    <Button
        android:id="@+id/EnviarNumele"
        android:layout_width="115dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="63dp"
        android:layout_marginTop="148dp"
        android:text="Enviar" />

    <Button
        android:id="@+id/EnviarSeparacion"
        android:layout_width="116dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/EnviarNumele"
        android:layout_alignTop="@+id/ETSeparacion"
        android:text="Enviar" />


    <Button
        android:id="@+id/IdDesconectar"
        android:layout_width="74dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="17dp"
        android:layout_marginEnd="112dp"
        android:background="@android:color/holo_orange_dark"
        android:text="Desconectar"
        android:textSize="9sp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="108dp"
        android:layout_height="54dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="17dp"
        android:adjustViewBounds="true"
        app:srcCompat="@drawable/junglelogojpg" />


    <TextView
        android:id="@+id/textoTXT"
        android:layout_width="133dp"
        android:layout_height="157dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="79dp"
        android:background="#E9E9E9"
        android:fontFamily="monospace"
        android:text="No hay entradas de resistividad"
        android:textColor="#000000" />


    <Button
        android:id="@+id/button3"
        android:layout_width="50dp"
        android:layout_height="32dp"
        android:layout_alignStart="@+id/EnviarNumele"
        android:layout_alignTop="@+id/Begin"
        android:background="#FF0000"
        android:baselineAligned="false"
        android:text="PARAR"
        android:textColor="@android:color/background_light"
        tools:ignore="RtlCompat" />


</RelativeLayout>