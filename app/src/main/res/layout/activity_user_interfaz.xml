<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".userinterfaz.UserInterfaz"
    android:background="#FFFFFF"
    >


    <Button
        android:id="@+id/BatrasDispositivos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"

        android:layout_marginTop="31dp"
        android:background="#436827"
        android:text="Atras"
        android:textColor="#FFFFFF" />

    <Button
        android:id="@+id/BguardarTXT"
        android:layout_width="63dp"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/BiniciarEnvioRecepcion"
        android:layout_alignTop="@+id/BidDesconectar"
        android:background="#989898"
        android:text="Guardar"
        android:textColor="#FFFFFF"
        android:textSize="10sp" />


    <Button
        android:id="@+id/BiniciarEnvioRecepcion"
        android:layout_width="52dp"
        android:layout_height="32dp"
        android:layout_alignParentStart="true"
        android:layout_centerVertical="true"
        android:layout_marginStart="134dp"
        android:background="#436827"
        android:text="iniciar"
        android:textColor="#FFFFFF" />


    <EditText
        android:id="@+id/ETSeparacion"
        android:layout_width="109dp"
        android:layout_height="54dp"
        android:layout_above="@+id/BenviarNumele"
        android:layout_alignEnd="@+id/BiniciarEnvioRecepcion"
        android:layout_marginBottom="-148dp"
        android:background="#FFFFFF"
        android:digits="0123456789."
        android:hapticFeedbackEnabled="false"
        android:hint="separaciÃ³n"
        android:inputType="numberDecimal"
        android:textColor="#436827"
        android:textColorHint="#436827"
        android:textSize="15sp" />


    <EditText
        android:id="@+id/ETnumele"
        android:layout_width="wrap_content"
        android:layout_height="51dp"
        android:layout_alignEnd="@+id/BiniciarEnvioRecepcion"
        android:layout_alignParentTop="true"
        android:layout_marginTop="160dp"
        android:background="#FFFFFF"
        android:digits="0123456789."
        android:hapticFeedbackEnabled="false"
        android:hint=" num electrodos"
        android:inputType="numberDecimal"
        android:textColor="#436827"
        android:textColorHint="#436827"
        android:textSize="15sp" />

    <Button
        android:id="@+id/BenviarNumele"
        android:layout_width="115dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="63dp"
        android:layout_marginTop="148dp"
        android:text="Enviar" />

    <Button
        android:id="@+id/BenviarSeparacion"
        android:layout_width="116dp"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/ETSeparacion"
        android:layout_alignStart="@+id/BenviarNumele"
        android:text="Enviar" />


    <Button
        android:id="@+id/BidDesconectar"
        android:layout_width="74dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="17dp"
        android:layout_marginEnd="112dp"
        android:background="@android:color/holo_orange_dark"
        android:text="Desconectar"
        android:textSize="9sp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="108dp"
        android:layout_height="54dp"
        android:layout_alignEnd="@+id/BidDesconectar"
        android:layout_alignParentTop="true"
        android:layout_marginTop="21dp"
        android:adjustViewBounds="true"
        app:srcCompat="@drawable/junglelogojpg" />


    <TextView
        android:id="@+id/TVresistividades"
        android:layout_width="133dp"
        android:layout_height="157dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="79dp"
        android:background="#E9E9E9"
        android:fontFamily="monospace"
        android:text="No hay entradas de resistividad"
        android:textColor="#000000" />


    <Button
        android:id="@+id/button3"
        android:layout_width="50dp"
        android:layout_height="32dp"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/BiniciarEnvioRecepcion"
        android:layout_marginEnd="103dp"
        android:background="#FF0000"
        android:baselineAligned="false"
        android:text="PARAR"
        android:textColor="@android:color/background_light"
        tools:ignore="RtlCompat" />


</RelativeLayout>